Global $Pause = True


HotKeySet("a", 'abcdefghi')

Func abcdefghi()
   HotKeySet("a")
   Send("a")
   If $Pause Then
      Sleep(20)
      Send("{ENTER}")
   EndIf
   HotKeySet("a", 'abcdefghi')
EndFunc


